<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_form_onboarding_inherit" model="ir.ui.view">
        <field name="name">Company onboard group</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Onboarding Panel">
                    <group>
                        <group>
                            <field name="account_invoice_onboarding_state"/>
                            <field name="account_dashboard_onboarding_state"/>
                            <field name="payment_acquirer_onboarding_state"/>
                            <field name="sale_quotation_onboarding_state"/>

                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>